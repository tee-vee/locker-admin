			<xs:documentation>UID = COM000179, Dictionary Entry Name = Chin Unit. Unit No. Text,
         Unit number (including any non-numeric parts) (e.g. 'A1', '3C')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="23"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ChinUnitUnitPortionText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000180, Dictionary Entry Name = Chin Unit. Unit Portion. Text,
         Indicate the concerned portion of a Unit in Chinese (e.g. '前座', '後座'). Used when the address refers to part of a Unit only</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="6"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="BuildingCSUInformationDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000181, Dictionary Entry Name = Building CSU Information. Details,
         Information related to the building of the concerned address, including an common identifier and the status of the building</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CsuId" type="BuildingCSUInformationCSUIDText.V1.0.CT"/>
			<xs:element name="Status" type="BuildingCSUInformationStatusCode.V1.0.CT" minOccurs="0"/>
			<xs:element name="StatusDate" type="BuildingCSUInformationStatusDate.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BuildingCSUInformationCSUIDText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000182, Dictionary Entry Name = Building CSU Information. CSU ID. Text,
         Building CSU ID, a unique and persistent identifier for on-ground buildings, formed by concatenating Geo Ref No, Polygon Type and Record Creation Date</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:pattern value="[0-9]{10}[P,T]{1}[0-9]{8}"/>
				<xs:length value="19"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="BuildingCSUInformationStatusCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000183, Dictionary Entry Name = Building CSU Information. Status. Code,
         Status of the concerned building, where P = Proposed, A = Active, D = Demolished</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="1"/>
				<xs:enumeration value="P"/>
				<xs:enumeration value="A"/>
				<xs:enumeration value="D"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="BuildingCSUInformationStatusDate.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000184, Dictionary Entry Name = Building CSU Information. Status. Date,
         The date when the status of the concerned building (i.e. value of the Status element) was last obtained</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Date.CT"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="PostBoxAddressDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000185, Dictionary Entry Name = Post Box Address. Details,
         Reference to an address of a Post Office Box</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EngPostBox" type="EngPostBoxDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="ChiPostBox" type="ChinPostBoxDetails.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngPostBoxDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000188, Dictionary Entry Name = Eng Post Box. Details,
         Entity for showing Post Office Box address</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PoBoxNo" type="EngPostBoxPOBoxNoNumeric.V1.0.CT"/>
			<xs:element name="PostOffice" type="EngPostBoxPostOfficeText.V1.0.CT"/>
			<xs:element name="PostOfficeRegion" type="EngPostBoxPostOfficeRegionText.V1.0.CT"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngPostBoxPOBoxNoNumeric.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000189, Dictionary Entry Name = Eng Post Box. PO Box No. Numeric,
         Post Office box number of a Post Box Address</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Numeric.CT">
				<xs:totalDigits value="6"/>
				<xs:fractionDigits value="0"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngPostBoxPostOfficeText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000190, Dictionary Entry Name = Eng Post Box. Post Office. Text,
         Name of the post office in English that the Post Office Box locates in.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngPostBoxPostOfficeRegionText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000191, Dictionary Entry Name = Eng Post Box. Post Office Region. Text,
         Name of the region in English that the concerned Post office locates in.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ChinPostBoxDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000192, Dictionary Entry Name = Chin Post Box. Details,
         Entity for showing Post Office Box address in Chinese</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PoBoxNo" type="ChinPostBoxPOBoxNoNumeric.V1.0.CT"/>
			<xs:element name="PostOffice" type="ChinPostBoxPostOfficeText.V1.0.CT"/>
			<xs:element name="PostOfficeRegion" type="ChinPostBoxPostOfficeRegionText.V1.0.CT"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ChinPostBoxPOBoxNoNumeric.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000193, Dictionary Entry Name = Chin Post Box. PO Box No. Numeric,
         Post Office box number of a Post Box Address</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Numeric.CT">
				<xs:totalDigits value="6"/>
				<xs:fractionDigits value="0"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ChinPostBoxPostOfficeText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000194, Dictionary Entry Name = Chin Post Box. Post Office. Text,
         Name of the post office in Chinese that the Post Office Box locates in.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="15"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ChinPostBoxPostOfficeRegionText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000195, Dictionary Entry Name = Chin Post Box. Post Office Region. Text,
         Name of the region in Chinese that the concerned Post office locates in.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="5"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ValidationInformationDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000196, Dictionary Entry Name = Validation Information. Details,
         Validation result of the address.  This element is present only when the address has ever been validated against an instance of the Common Address Database of the Government.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LevelPassed" type="ValidationInformationLevelPassedCode.V1.0.CT" minOccurs="0"/>
			<xs:element name="ValidationTime" type="ValidationInformationValidationTimeDateTime.V1.0.CT" minOccurs="0"/>
			<xs:element name="Premises2dAddressId" type="ValidationInformationPremises2DAddressIDNumeric.V1.0.CT" minOccurs="0"/>
			<xs:element name="UnitId" type="ValidationInformationUnitIDNumeric.V1.0.CT" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ValidationInformationLevelPassedCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000197, Dictionary Entry Name = Validation Information. Level Passed. Code,
         The level of validation passed for the concerned address.  This can be:
2D = Validation up to 2D level (Without floor and unit information), 
Floor = Validation up to floor level, 
Unit = Validation up to unit level (Fully validated),
Fail = The concerned address cannot be validated</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="5"/>
				<xs:enumeration value="2D"/>
				<xs:enumeration value="Floor"/>
				<xs:enumeration value="Unit"/>
				<xs:enumeration value="Fail"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ValidationInformationValidationTimeDateTime.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000198, Dictionary Entry Name = Validation Information. Validation Time. Date Time,
         Date and time when the concerned address is validated</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:DateTime.CT"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ValidationInformationPremises2DAddressIDNumeric.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000199, Dictionary Entry Name = Validation Information. Premises 2D Address ID. Numeric,
         Unique identifier of the corresponding 2D Address record in the Common Address Database of the Government</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Numeric.CT">
				<xs:totalDigits value="9"/>
				<xs:fractionDigits value="0"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ValidationInformationUnitIDNumeric.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000200, Dictionary Entry Name = Validation Information. Unit ID. Numeric,
         Unique identifier of the corresponding Unit record in the Common Address Database of the Government</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Numeric.CT">
				<xs:totalDigits value="9"/>
				<xs:fractionDigits value="0"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>Document root elements</xs:documentation>
	</xs:annotation>
	<xs:element name="StructuredAddress" type="StructuredAddressDetails.V1.0.CT"/>
</xs:schema>
