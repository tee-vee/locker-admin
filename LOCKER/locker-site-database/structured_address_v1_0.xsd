<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cct="http://www.xml.gov.hk/schemas/cct">
	<xs:import namespace="http://www.xml.gov.hk/schemas/cct" 
		schemaLocation="http://www.xml.gov.hk/schemas/cct/cct.xsd"/>
	<xs:complexType name="StructuredAddressDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000046, Dictionary Entry Name = Structured Address. Details,
         Contain necessary information about a structured (possibly validated) address within Hong Kong, optionally together with validation result of the address.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Address" type="AddressDetails.V1.0.CT"/>
			<xs:element name="ValidationInformation" type="ValidationInformationDetails.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000049, Dictionary Entry Name = Address. Details,
         Specified an postal address</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:annotation><xs:documentation>xs:choice</xs:documentation></xs:annotation>
			<xs:element name="PremisesAddress" type="PremisesAddressDetails.V1.0.CT"/>
			<xs:element name="PostBoxAddress" type="PostBoxAddressDetails.V1.0.CT"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="PremisesAddressDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000052, Dictionary Entry Name = Premises Address. Details,
         Reference to an address which can be occupied by persons or organizations</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EngPremisesAddress" type="EngPremisesAddressDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="ChiPremisesAddress" type="ChinPremisesAddressDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingCsuInformation" type="BuildingCSUInformationDetails.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngPremisesAddressDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000056, Dictionary Entry Name = Eng Premises Address. Details,
         A structured postal address in English referring to a location that can be occupied by persons or organizations within Hong Kong</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Eng3dAddress" type="Eng3DAddressDetails.V1.0.CT" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="EngBlock" type="EngBlockDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingName" type="EngPremisesAddressBuildingNameText.V1.0.CT" minOccurs="0"/>
			<xs:element name="EngEstate" type="EngEstateDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="EngVillage" type="EngVillageDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="EngStreet" type="EngStreetDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="EngLot" type="EngLotDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="EngDistrict" type="EngDistrictDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="Region" type="EngPremisesAddressRegionCode.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngPremisesAddressBuildingNameText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000059, Dictionary Entry Name = Eng Premises Address. Building Name. Text,
         Full building name in English</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="195"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngPremisesAddressRegionCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000065, Dictionary Entry Name = Eng Premises Address. Region. Code,
         Region that the address reference to, where 
HK = Hong Kong Island, 
KLN = Kowloon, 
NT = New Territories</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="3"/>
				<xs:enumeration value="HK"/>
				<xs:enumeration value="KLN"/>
				<xs:enumeration value="NT"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="Eng3DAddressDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000066, Dictionary Entry Name = Eng 3D Address. Details,
         Floor and Unit information of the concerned address in English.  If multiple floors and/or units are concerned in a single address, each concerned floor and unit pair should be recorded as one Eng 3D Address element.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EngUnit" type="EngUnitDetails.V1.0.CT" minOccurs="0"/>
			<xs:element name="EngFloor" type="EngFloorDetails.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngUnitDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000069, Dictionary Entry Name = Eng Unit. Details,
         Unit information of the concerned address in English</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="UnitDescriptor" type="EngUnitUnitDescriptorText.V1.0.CT" minOccurs="0"/>
			<xs:element name="UnitNo" type="EngUnitUnitNoText.V1.0.CT"/>
			<xs:element name="UnitPortion" type="EngUnitUnitPortionCode.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngUnitUnitDescriptorText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000070, Dictionary Entry Name = Eng Unit. Unit Descriptor. Text,
         Unit descriptor in English (e.g. 'FLAT', 'SHOP')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="16"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngUnitUnitNoText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000071, Dictionary Entry Name = Eng Unit. Unit No. Text,
         Unit number (including any non-numeric parts) (e.g. 'A1', '3C')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="23"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngUnitUnitPortionCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000072, Dictionary Entry Name = Eng Unit. Unit Portion. Code,
         Indicate the concerned portion of a Unit in English, where: 
CE = Central, 
E = East, 
FR = Front, 
LF = Left, 
MD = Middle, 
N = North, 
NE = North-East, 
NW = North-West, 
RG = Right, 
RR = Rear, 
S = South, 
SE = South-East, 
SW = South-West, 
W = West, 
PN = (Portion), 
PT = (Part), 
RF = &amp; Roof
Used when the address refers to part of a Unit only</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="2"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngFloorDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000073, Dictionary Entry Name = Eng Floor. Details,
         Floor information of the concerned address in English</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:annotation><xs:documentation>xs:choice</xs:documentation></xs:annotation>
			<xs:element name="FloorNum" type="EngFloorFloorNumNumeric.V1.0.CT"/>
			<xs:element name="FloorDescription" type="EngFloorFloorDescriptionText.V1.0.CT"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="EngFloorFloorNumNumeric.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000074, Dictionary Entry Name = Eng Floor. Floor Num. Numeric,
         Floor number, used only when the floor information is a numeric value.  Floor description in English as in full postal address can be generated by appending '/F' to FloorNum.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Numeric.CT">
				<xs:totalDigits value="3"/>
				<xs:fractionDigits value="0"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngFloorFloorDescriptionText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000075, Dictionary Entry Name = Eng Floor. Floor Description. Text,
         English floor description, used only when the floor information contains non-numeric part
e.g. 'CONCOURSE', 'LEVEL 1', 'UPPER GROUND', '12A/F'</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngBlockDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000076, Dictionary Entry Name = Eng Block. Details,
         Block information of the concerned address in English</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BlockDescriptor" type="EngBlockBlockDescriptorText.V1.0.CT" minOccurs="0"/>
			<xs:element name="BlockNo" type="EngBlockBlockNoText.V1.0.CT"/>
			<xs:element name="BlockDescriptorPrecedenceIndicator" type="EngBlockBlockDescriptorPrecedenceIndicator.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngBlockBlockDescriptorText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000077, Dictionary Entry Name = Eng Block. Block Descriptor. Text,
         Block descriptor in English (e.g. 'Building', 'House', 'Block', 'BLK')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="35"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngBlockBlockNoText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000078, Dictionary Entry Name = Eng Block. Block No. Text,
         Block number in English or numeric values (e.g. '1', 'A', 'EAST')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="15"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngBlockBlockDescriptorPrecedenceIndicator.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000079, Dictionary Entry Name = Eng Block. Block Descriptor Precedence. Indicator,
         An indicator showing the ordering of block descriptor and block number in full English address when both are not Null, where 
Y = block description precedes block number (e.g. in the cases of 'BLOCK A' and 'BLOCK 1'), 
N = block number precedes block description (e.g. in the cases of 'NORTH BLOCK' and 'WEST TOWER')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Indicator.CT">
				<xs:maxLength value="1"/>
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngEstateDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000080, Dictionary Entry Name = Eng Estate. Details,
         Estate name and related information of the concerned address in English</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EstateName" type="EngEstateEstateNameText.V1.0.CT"/>
			<xs:element name="EngPhase" type="EngPhaseDetails.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngEstateEstateNameText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000081, Dictionary Entry Name = Eng Estate. Estate Name. Text,
         The estate name of the address in English</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="80"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngPhaseDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000083, Dictionary Entry Name = Eng Phase. Details,
         Phase name and phase number information of the concerned address in English</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhaseName" type="EngPhasePhaseNameText.V1.0.CT" minOccurs="0"/>
			<xs:element name="PhaseNo" type="EngPhasePhaseNoNumeric.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngPhasePhaseNameText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000084, Dictionary Entry Name = Eng Phase. Phase Name. Text,
         The phase name in English of the concerned address</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="80"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngPhasePhaseNoNumeric.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000085, Dictionary Entry Name = Eng Phase. Phase No. Numeric,
         The phase number of the concerned address</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Numeric.CT">
				<xs:totalDigits value="2"/>
				<xs:fractionDigits value="0"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngVillageDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000086, Dictionary Entry Name = Eng Village. Details,
         Village name and building number information of the concerned address in English</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VillageName" type="EngVillageVillageNameText.V1.0.CT"/>
			<xs:element name="BuildingNoFrom" type="EngVillageBuildingNoFromText.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoTo" type="EngVillageBuildingNoToText.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoOddEvenIndicator" type="EngVillageBuildingNoOddEvenIndicatorCode.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoUnofficialIndicator" type="EngVillageBuildingNoUnofficialIndicatorCode.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoConfirmationCode" type="EngVillageBuildingNoConfirmationCode.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngVillageVillageNameText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000087, Dictionary Entry Name = Eng Village. Village Name. Text,
         Full village/place name of the address in English</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="67"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngVillageBuildingNoFromText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000088, Dictionary Entry Name = Eng Village. Building No From. Text,
         Starting building number of the address, or the full building number when a single building number value is concerned, non-numeric part is included (e.g. '12A' in '12A-14B', and '3B' in '3B/3C')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:pattern value="[A-Z]{0,2}[0-9]{0,4}[A-Z]{0,2}"/>
				<xs:maxLength value="7"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngVillageBuildingNoToText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000089, Dictionary Entry Name = Eng Village. Building No To. Text,
         Ending building number of the address, non-numeric part is included (e.g. '12B' in '12A-14B', and '3C' in '3B/3C')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:pattern value="[A-Z]{0,2}[0-9]{0,4}[A-Z]{0,2}"/>
				<xs:maxLength value="7"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngVillageBuildingNoOddEvenIndicatorCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000090, Dictionary Entry Name = Eng Village. Building No Odd Even Indicator. Code,
         Indicator showing the building numbers concerned within the range, where 
O = Odd numbers within the range, 
E = Even numbers within the range, 
B = Both even and odd numbers within the range</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="1"/>
				<xs:enumeration value="O"/>
				<xs:enumeration value="E"/>
				<xs:enumeration value="B"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngVillageBuildingNoUnofficialIndicatorCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000091, Dictionary Entry Name = Eng Village. Building No Unofficial Indicator. Code,
         Indicator showing the unofficial part(s) of address, where 
1 = Unofficial building number or no building number, 
2 = Unofficial building number and village name</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="1"/>
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngVillageBuildingNoConfirmationCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000092, Dictionary Entry Name = Eng Village. Building No Confirmation. Code,
         Indicator showing whether the assigned building number is confirmed, where 
C = Confirmed building number, 
P = Provisional building number, 
A = Not yet allocated but action to be taken, 
B = Use existing old number but action to be taken, 
X = Not applicable.
When this code is C or P, the 'Building No From' must not be Null</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="1"/>
				<xs:enumeration value="C"/>
				<xs:enumeration value="P"/>
				<xs:enumeration value="A"/>
				<xs:enumeration value="B"/>
				<xs:enumeration value="X"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngStreetDetails.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000093, Dictionary Entry Name = Eng Street. Details,
         Street name and building number information of the concerned address in English</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StreetName" type="EngStreetStreetNameText.V1.0.CT"/>
			<xs:element name="BuildingNoFrom" type="EngStreetBuildingNoFromText.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoTo" type="EngStreetBuildingNoToText.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoOddEvenIndicator" type="EngStreetBuildingNoOddEvenIndicatorCode.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoUnofficialIndicator" type="EngStreetBuildingNoUnofficialIndicatorCode.V1.0.CT" minOccurs="0"/>
			<xs:element name="BuildingNoConfirmationCode" type="EngStreetBuildingNoConfirmationCode.V1.0.CT" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EngStreetStreetNameText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000094, Dictionary Entry Name = Eng Street. Street Name. Text,
         Full street name of the address in English</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:maxLength value="67"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngStreetBuildingNoFromText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000095, Dictionary Entry Name = Eng Street. Building No From. Text,
         Starting building number of the address, or the full building number when a single building number value is concerned, non-numeric part is included (e.g. '12A' in '12A-14B', and '3B' in '3B/3C')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:pattern value="[A-Z]{0,2}[0-9]{0,4}[A-Z]{0,2}"/>
				<xs:maxLength value="7"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngStreetBuildingNoToText.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000096, Dictionary Entry Name = Eng Street. Building No To. Text,
         Ending building number of the address, non-numeric part is included (e.g. '12B' in '12A-14B', and '3C' in '3B/3C')</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Text.CT">
				<xs:pattern value="[A-Z]{0,2}[0-9]{0,4}[A-Z]{0,2}"/>
				<xs:maxLength value="7"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngStreetBuildingNoOddEvenIndicatorCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000097, Dictionary Entry Name = Eng Street. Building No Odd Even Indicator. Code,
         Indicator showing the building numbers concerned within the range, where 
O = Odd numbers within the range, 
E = Even numbers within the range, 
B = Both even and odd numbers within the range</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="1"/>
				<xs:enumeration value="O"/>
				<xs:enumeration value="E"/>
				<xs:enumeration value="B"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngStreetBuildingNoUnofficialIndicatorCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000098, Dictionary Entry Name = Eng Street. Building No Unofficial Indicator. Code,
         Indicator showing the unofficial part(s) of address, where 
1 = Unofficial building number or no building number, 
2 = Unofficial building number and building name</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:Code.CT">
				<xs:maxLength value="1"/>
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="EngStreetBuildingNoConfirmationCode.V1.0.CT">
		<xs:annotation>
			<xs:documentation>UID = COM000099, Dictionary Entry Name = Eng Street. Building No Confirmation. Code,
         Indicator showing whether the assigned building number is confirmed, where 
C = Confirmed building number, 
P = Provisional building number, 
A = Not yet allocated but action to be taken, 
B = Use existing old number but action to be taken, 
X = Not applicable.
When this code is C or P, the 'Building No From' must not be Null</xs:documentation>
		</xs:annotation>
